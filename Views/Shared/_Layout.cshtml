
        <!DOCTYPE html>
        <html lang="en">
        <head>

            <!-- Basic Page Needs
            ================================================== -->
            <meta charset="utf-8">
            <title>@ViewData["Title"] - AGECorporate</title>

            <!-- Mobile Specific Metas
            ================================================== -->
            <meta http-equiv="X-UA-Compatible" content="IE=edge">
            <meta name="description" content="Construction Html5 Template">
            <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=5.0">

            <!-- CSS
            ================================================== -->
            <!-- Bootstrap -->
            <link rel="stylesheet" href="~/plugins/bootstrap/bootstrap.min.css">
            <!-- FontAwesome -->
            <link rel="stylesheet" href="~/plugins/fontawesome/css/all.min.css">
            <!-- Animation -->
            <link rel="stylesheet" href="~/plugins/animate-css/animate.css">
            <!-- slick Carousel -->
            <link rel="stylesheet" href="~/plugins/slick/slick.css">
            <link rel="stylesheet" href="~/plugins/slick/slick-theme.css">
            <!-- Colorbox -->
            <link rel="stylesheet" href="~/plugins/colorbox/colorbox.css">
            <!-- Template styles-->
            <link rel="stylesheet" href="~/css/style.css">

        </head>
        <body>
            <div class="body-inner">


                <!-- Header start -->
                <header id="header" class="header-one">
                    <div class="bg-white">
                        <div class="container">
                            <div class="logo-area">
                                <div class="row align-items-center">
                                    <div class="logo col-lg-3 text-center text-lg-left mb-3 mb-md-5 mb-lg-0">
                                        <a class="d-block" asp-action="Index">
                                            <h1 style="font-size:3rem">AGE</h1>
                                        </a>
                                    </div><!-- logo end -->

                                    <div class="col-lg-9 header-right">
                                        <ul class="top-info-box">
                                            <li>
                                                <div class="info-box">
                                                    <div class="info-box-content">
                                                        <p class="info-box-title">Call Us</p>
                                                <p class="info-box-subtitle"><a href="tel:+923333065301">(+92) 333 3065 301</a></p>
                                                    </div>
                                                </div>
                                            </li>
                                            <li>
                                                <div class="info-box">
                                                    <div class="info-box-content">
                                                        <p class="info-box-title">Email Us</p>
                                                        <p class="info-box-subtitle"><a href="mailto:info@alghazalienterprises.com">info@alghazalienterprises.com</a></p>
                                                    </div>
                                                </div>
                                            </li>
                                        </ul><!-- Ul end -->
                                    </div><!-- header right end -->
                                </div><!-- logo area end -->

                            </div><!-- Row end -->
                        </div><!-- Container end -->
                    </div>

                </header>
                <!--/ Header end -->

        <div id="banner-area" class="banner-area" style="background-image:url('@Url.Content("~/images/banner/fibre-banner.jpg")?v=@DateTime.Now.Ticks');           background-size: cover;
            background-position: center;
            position: relative;
            display: flex;
            align-items: center;
            justify-content: center;">
            <div style="position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                background-color: rgba(0, 0, 0, 0.2);">
            </div>
              <div class="banner-text">
                     <div class="container">
                         <div class="row">
                           <div class="col-lg-12">
                               <div class="banner-heading">
                                 <h1 class="banner-title">@ViewData["Title"]</h1>
                               </div>
                           </div><!-- Col end -->
                         </div><!-- Row end -->
                     </div><!-- Container end -->
                   </div><!-- Banner text end -->
                 </div><!-- Banner area end --> 

                <!-- #Body -->
                    @RenderBody()

                <!-- Footer start -->
                <footer id="footer" class="footer bg-overlay mt-5">
                    <div class="footer-main">
                        <div class="container">
                            <div class="row justify-content-between">
                                <div class="col-lg-7 col-md-9 col-sm-11 mx-auto footer-widget footer-about">
                                    <h3 class="widget-title">About Us</h3>
                                    <h1 class="text-light">AGE</h1>
                                    <p>
                                        Al Ghazali Enterprises is committed to delivering excellence in every aspect of our business. We pride ourselves on building strong relationships with our clients and delivering solutions that exceed expectations.
                                    </p>
                                </div><!-- Col end -->

                            </ul>
                                </div><!-- Col end -->
                            </div><!-- Row end -->
                        </div><!-- Container end -->
                    </div><!-- Footer main end -->
                </footer>
                <!-- Footer end -->

